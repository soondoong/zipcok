<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="zipcok">
  
  
  	<select id = "totalCnt" parameterType="HashMap" >

	select count(*) from
    (select rownum as rnum,a.* from
    (select * from zipcok_member
     where
     mem_type like
    <choose>
        <when test = "type =='전체'">'%%'</when>
        <when test = "type !='전체'"> '%${type}%'</when>
    </choose>

    )a)b
    
	</select>
  
  
	<select id="adminMemberList" parameterType="HashMap" resultType="zipcok.member.model.MemberDTO">
	
	select * from
    (select rownum as rnum,a.* from
    (select * from zipcok_member
     where
     mem_type like
    <choose>
        <when test = "type =='전체'">'%%'</when>
        <when test = "type !='전체'"> '%${type}%'</when>
    </choose>
 )a)b
    
	</select>

  </mapper>